{
  "name": "@huddle01/web-core",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "types.d.ts"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup",
    "watch": "tsup --watch",
    "pub": "yarn build && npm publish --access public",
    "pub:beta": "yarn build && npm publish --access public --tag beta",
    "pub:alpha": "npm publish --access public --tag alpha",
    "pub:dev": "npm publish --access public --tag dev",
    "test": "vitest",
    "test:types": "vitest typecheck"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@vitest/browser": "^0.34.1",
    "esbuild": "^0.19.5",
    "jsdom": "^22.1.0",
    "protoc": "workspace:*",
    "tsconfig": "workspace:*",
    "tsup": "^7.2.0",
    "types": "workspace:*",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "webdriverio": "^8.15.4"
  },
  "dependencies": {
    "mediasoup-client": "^3.7.2",
    "zod": "^3.22.1"
  }
}
